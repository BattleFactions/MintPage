{
  "name": "mint-page",
  "version": "0.1.0",
  "private": true,
  "description": "Battle Factions NFTs Mint Page",
  "license": "MIT",
  "author": "Battle Factions Tech Army",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --colors",
    "test:ci": "npm run test:coverage && npm run lint",
    "test:auto": "jest --watch --verbose",
    "test:coverage": "jest --no-cache --coverage --silent",
    "ci": "start-server-and-test start:test http://localhost:5678 e2e",
    "start": "parcel serve ./public/index.html",
    "start:clean": "npm run clean && npm start",
    "start:test": "parcel serve ./public/index.html --port 5678",
    "start:dist": "npm run clean && npm run build && node localserver.js",
    "build": "parcel build ./public/index.html --public-url ./ --no-source-maps --no-cache --no-scope-hoist --detailed-report",
    "clean": "rimraf ./coverage && rimraf ./dist && rimraf ./.parcel-cache",
    "clean:all": "npm run clean && rimraf ./node_modules"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:ci"
    }
  },
  "config": {
    "prefer-offline": "true"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "dependencies": {
    "@rjsf/core": "^3.2.0",
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "grommet": "^2.18.0",
    "http-status-codes": "^2.1.4",
    "moment": "^2.29.1",
    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-idle-timer": "^4.6.4",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@parcel/core": "^2.0.0-nightly.903",
    "@parcel/runtime-react-refresh": "^2.0.0-nightly.905",
    "@parcel/transformer-react-refresh-wrap": "^2.0.0-nightly.905",
    "@parcel/transformer-typescript-tsc": "^2.0.0-nightly.905",
    "@parcel/validator-typescript": "^2.0.0-nightly.905",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/eslint": "^7.28.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/parcel-env": "^0.0.1",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/rimraf": "^3.0.2",
    "@types/styled-components": "^5.1.15",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__react-hooks": "^4.0.0",
    "@types/testing-library__user-event": "^4.2.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.17.1",
    "husky": "^4.3.8",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "serverless": "^2.64.1",
    "serverless-cloudfront-invalidate": "^1.11.0",
    "serverless-lambda-edge-pre-existing-cloudfront": "^1.2.0",
    "serverless-s3-sync": "^1.17.3",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "parcel": "^2.0.0-nightly.903"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "alias": {
    "app": "./src",
    "common": "./src/common",
    "react": "./node_modules/react",
    "react-dom": "./node_modules/react-dom",
    "@types/react": "./node_modules/@types/react",
    "@types/react-dom": "./node_modules/@types/react-dom",
    "@types/react-router": "./node_modules/@types/react-router",
    "@types/react-router-dom": "./node_modules/@types/react-router-dom"
  }
}
